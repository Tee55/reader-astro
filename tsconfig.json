{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/components/*": [
                "./src/components/*"
            ],
            "@/lib/*": [
                "./src/lib/*"
            ],
            "@/data/*": [
                "./src/data/*"
            ],
            "@/utils/*": [
                "./src/utils/*"
            ],
            "@/*": [
                "./src/*"
            ]
        }
    }
}